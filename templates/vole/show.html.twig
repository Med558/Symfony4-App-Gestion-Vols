{% extends 'adminlayout.html.twig' %}


{% block body %}
    <h1>Vole</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ vole.id }}</td>
            </tr>
            <tr>
                <th>Heurdepart</th>
                <td>{{ vole.heurdepart ? vole.heurdepart|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Heurearrive</th>
                <td>{{ vole.heurearrive ? vole.heurearrive|date('H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Compagnie</th>
                <td>{{ vole.compagnie.nom }}</td>
            </tr>
            <tr>
                <th>Aeroport_depart</th>
                <td>{{ vole.aeroportDepart }}</td>
            </tr>
            <tr>
                <th>Aeroport_arrive</th>
                <td>{{ vole.aeroportArrive }}</td>
            </tr>
           
        </tbody>
    </table>

    <a href="{{ path('vole_index') }}">back to list</a>

    <a href="{{ path('vole_edit', {'id': vole.id}) }}">edit</a>

    {{ include('vole/_delete_form.html.twig') }}
{% endblock %}
